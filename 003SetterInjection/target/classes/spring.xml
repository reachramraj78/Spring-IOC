<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	
	<bean id="address" class="com.balaji.bean.Address">
	       <property name="aid" value="1"></property>
	       <property name="aloc" value="SKLM"></property>
	</bean>
	<bean id="employee" class="com.balaji.bean.Employee">
		<property name="eid" value="1001"></property>
		<property name="ename" value="Balaji"></property>
		<property name="address" ref="address"></property>
	</bean>
</beans>